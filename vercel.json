{
  "buildCommand": "yarn workspace frontend build",
  "outputDirectory": "frontend/dist",
  "installCommand": "yarn install",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    }
  ]
}